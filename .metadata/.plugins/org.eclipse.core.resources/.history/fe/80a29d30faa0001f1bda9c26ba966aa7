package fr.insa.ms.userInfoService.model;

public class UserInfos {
	public enum HelpStatus {OFFERING_HELP, ASKING_FOR_HELP}
	
	private int id;
	private String birthDate;
	private String lastName;
	private String firstName;
	private String location;
	private HelpStatus helpStatus;

	
	public UserInfos() {}


	public UserInfos(int id, String birthdate, String lastName, String firstName, String location, HelpStatus helpStatus) {
	    this.id = id;
	    this.birthDate = birthDate;
	    this.lastName = lastName;
	    this.firstName = firstName;
	    this.location = location;
	    this.helpStatus = helpStatus;
	}
	public int getId(){
	    return id;
	}
	public void setId (int id) {
	    this.id = id;
	}
	public String getBirthDate() {
	    return birthDate;
	}
	public void setBirthDate(String birthDate) {
	    this.birthDate = birthDate;
	}
	public String getLastName() {
	    return lastName;
	}
	public void setLastName(String lastName) {
	    this.lastName = lastName;
	}
	public String getFirstName() {
	    return firstName;
	}
	public void setFirstName(String firstName) {
	    this.firstName = firstName;
	}
	public String getLocation() {
		return location;
	}
	public void setLocation(String location) {
		this.location = location;
	}
	public HelpStatus getHelpStatus() { 
		return helpStatus; 
		} 
	public void setHelpStatus(HelpStatus helpStatus) {
		this.helpStatus = helpStatus; 
		}

}
